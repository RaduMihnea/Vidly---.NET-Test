@model Test2.Models.Customer
@{
    ViewBag.Title = "Customer Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Membership: @Model.MembershipType.Name</li>
    @if (@Model.BirthDate.HasValue)
    {
        <li>Birth-Date: @Model.BirthDate.Value.ToShortDateString()</li>
    }
</ul>